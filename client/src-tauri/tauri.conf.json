{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "CodeCollab",
  "version": "0.1.0",
  "identifier": "com.codecollab.dev",
  "build": {
    "frontendDist": ".next",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm run dev",
    "beforeBuildCommand": "pnpm run build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "CodeCollab - Real-time Collaboration",
        "width": 1400,
        "height": 900,
        "minWidth": 1024,
        "minHeight": 700,
        "resizable": true,
        "fullscreen": false,
        "center": true
      },
      {
        "label": "maintwo",
        "title": "CodeCollab - Real-time Collaboration -2",
        "width": 1400,
        "height": 900,
        "minWidth": 1024,
        "minHeight": 700,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
