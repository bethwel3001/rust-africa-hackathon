{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Enables permissions for CodeCollab collaboration features",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-exists",
    "fs:allow-stat",
    "shell:allow-open",
    "shell:allow-execute",
    "http:default",
    "http:allow-fetch",
    "http:allow-fetch-cancel",
    "http:allow-fetch-read-body",
    "http:allow-fetch-send",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "websocket:default",
    "websocket:allow-connect",
    "websocket:allow-send"
  ]
}
